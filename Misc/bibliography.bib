@article{Bickman2005,
abstract = {Two large-area, low noise, high speed fluorescence detectors have been built. One detector consists of a photodiode with an area of 28 mm x 28 mm and a low noise transimpedance amplifier. This detector has a input light-equivalent spectral noise density of less than 3 pW/Hz{\^{}}1/2, can recover from a large scattered light pulse within 10 us, and has a bandwidth of at least 900 kHz. The second detector consists of a 16 mm diameter avalanche photodiode and a low-noise transimpedance amplifier. This detector has an input light-equivalent spectral noise density of 0.08 pW/Hz{\^{}}1/2, also can recover from a large scattered light pulse within 10 us, and has a bandwidth of 1 MHz.},
