@article{Han2006,
abstract = {This letter presents analysis and design considerations for lumped (inductor and capacitor) matching networks operating at high efficiency ({\&}gt;95{\%}). Formulas for calculating matching network efficiency are given, and it is shown that efficiency can be expressed as a function of inductor quality factor Q{\textless}sub{\textgreater}L{\textless}/sub{\textgreater}, capacitor quality factor Q{\textless}sub{\textgreater}C {\textless}/sub{\textgreater}, and transformation ratio. These formulas are used to evaluate the optimum number of L-section matching stages as a function of conversion ratio. Both simulation and experimental results are presented that validate the analytical formulation},
