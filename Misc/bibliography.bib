journal = {2010 IEEE 12th Workshop on Control and Modeling for Power Electronics, COMPEL 2010},
keywords = {Charge-pump,High power density,Switched Capacitor DC-DC converters},
title = {{A comparative analysis of Switched-Capacitor and inductor-based DC-DC conversion technologies}},
year = {2010}
}
