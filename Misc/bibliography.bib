issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
number = {22},
pages = {201403002},
pmid = {24843161},
title = {{Wireless power transfer to deep-tissue microimplants}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1403002111{\%}5Cnpapers3://publication/doi/10.1073/pnas.1403002111},
