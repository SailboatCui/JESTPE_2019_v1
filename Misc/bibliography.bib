@article{Hooper2011,
abstract = {Our development of an Automated Test System (ATS) for Proximity Sensor Electronic Units (PSEUs) in the aircraft industry required the implementation of a variable inductor. The variable inductor simulates inductive proximity sensors so the ATS can measure and verify the switch points of the electronics as the sensors move between their near and far states. Though developed for PSEU testing, the presented methodology and technology may be applied to other test equipment and applications that require a variable inductor. The paper begins by looking at different techniques for implementing a variable inductor: moving cores, switched decade boxes, gyrator circuitry and saturable core reactors. The paper presents the pros and cons of the different technologies and then focuses on the development of a saturable core reactor as the chosen technology. The paper presents fundamental formulae used during the development of the variable inductor and test results for a number of developed prototypes. The presentation includes the development of a highly-accurate control loop to precisely hold the value of the controlled inductance. Finally, the paper concludes with a brief discussion of the ATS that ultimately housed the variable inductor.},
author = {Hooper, Rich and Guy, Bill and Perrault, Rick},
doi = {10.1109/MIM.2011.5961370},
file = {:C$\backslash$:/Users/richa/Google Drive (cuixf@umich.edu)/Reference/Hooper, Guy, Perrault/IEEE Instrumentation and Measurement Magazine/Hooper, Guy, Perrault - 2011 - IEEE Instrumentation and Measurement Magazine.pdf:pdf},
isbn = {978-1-4244-7960-3},
issn = {10946969},
journal = {IEEE Instrumentation and Measurement Magazine},
keywords = {-proximity sensor electronics unit,saturable core reactor automated,test equipment,variable inductor},
number = {4},
pages = {39--44},
publisher = {IEEE},
title = {{A current-controlled variable inductor}},
volume = {14},
year = {2011}
}
@article{Brunoro1999,
abstract = {This paper presents a high-performance DC-DC switching mode power supply designed to deliver a regulated 0-50 V/0-10 A output. The proposed power supply is based on a modified version of the zero-voltage switching (ZVS) full-bridge (FB) phase-shift DC-DC converter, which incorporates commutation auxiliary inductors to provide ZVS for the entire load range as well as a commutation aid circuit to clamp the output diode voltage. The control strategy is based on two control loops operating in cascade mode. The inner loop maintains a regulated output current, whereas the external voltage loop regulates the output voltage, independently of load and input-voltage changes. In order to obtain a high-reliability converter, the control circuit has been implemented using just two integrated circuits (ICs). The phase-shift regulator UC3875 IC generates the gate drive signal to the MOSFET's. The control loop regulators are implemented using the TL074 IC. A theoretical analysis was conducted, and experimental results were obtained for a 0-50 V/0-10 A power supply operating at 100 kHz},
author = {Brunoro, Marcelo and Vieira, Jos{\'{e}} Luiz F},
doi = {10.1109/63.761693},
file = {:C$\backslash$:/Users/richa/Google Drive (cuixf@umich.edu)/Reference/Brunoro, Vieira/IEEE Transactions on Power Electronics/Brunoro, Vieira - 1999 - IEEE Transactions on Power Electronics.pdf:pdf},
issn = {08858993},
journal = {IEEE Transactions on Power Electronics},
keywords = {Adjustable power supply,High-performance dc-dc converter,ZVS converter},
number = {3},
pages = {495--505},
title = {{A high-performance ZVS full-bridge DC-DC 0-50-V/0-10-A power supply with phase-shift control}},
volume = {14},
year = {1999}
}
@article{Niroui2015,
abstract = {Abrupt switching behavior and near-zero leakage current of nanoelectromech. (NEM) switches are advantageous properties through which NEMs can outperform conventional semiconductor elec. switches.  To date, however, typical NEMs structures require high actuation voltages and can prematurely fail through permanent adhesion (defined as stiction) of device components.  To overcome these challenges, the authors propose a NEM switch, termed a squitch, which is designed to electromechanically modulate the tunneling current through a nanometer-scale gap defined by an org. mol. film sandwiched between two electrodes.  When voltage is applied across the electrodes, the generated electrostatic force compresses the sandwiched mol. layer, thereby reducing the tunneling gap and causing an exponential increase in the current through the device.  The presence of the mol. layer avoids direct contact of the electrodes during the switching process.  Also, as the layer is compressed, the increasing surface adhesion forces are balanced by the elastic restoring force of the deformed mols. which can promote zero net stiction and recoverable switching.  Through numerical anal., the authors demonstrate the potential of optimizing squitch design to enable large on-off ratios beyond 6 orders of magnitude with operation in the sub-1 V regime and with nanoseconds switching times.  The preliminary exptl. results based on metal-mol.-graphene devices suggest the feasibility of the proposed tunneling switching mechanism.  With optimization of device design and material engineering, squitches can give rise to a broad range of low-power electronic applications. [on SciFinder(R)]},
author = {Niroui, Farnaz and Wang, Annie I. and Sletten, Ellen M. and Song, Yi and Kong, Jing and Yablonovitch, Eli and Swager, Timothy M. and Lang, Jeffrey H. and Bulovi??, Vladimir},
doi = {10.1021/acsnano.5b02476},
file = {:C$\backslash$:/Users/richa/Google Drive (cuixf@umich.edu)/Reference/Niroui et al/ACS Nano/Niroui et al. - 2015 - ACS Nano.pdf:pdf},
issn = {1936086X},
journal = {ACS Nano},
keywords = {Simmons model,graphene,nanoelectromechanical (NEM) switches,organic molecular thin film,quantum tunneling,self-assembled monolayer,stiction},
number = {8},
pages = {7886--7894},
title = {{Tunneling Nanoelectromechanical Switches Based on Compressible Molecular Thin Films}},
volume = {9},
year = {2015}
}
