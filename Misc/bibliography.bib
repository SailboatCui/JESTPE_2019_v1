isbn = {9781467378857},
journal = {3rd IEEE workshop on Wide Band Gap Devices and Applications},
keywords = {7,8,a conventional comparison is,and,bidirectional converter,fom,gan devices,influence on,isolated dc-dc,it is,loss analysis,pointed that conduction loss,presented in,should have a greater,synchronous rectifying mode},
