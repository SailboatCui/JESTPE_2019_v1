isbn = {9781457712159},
journal = {Conference Proceedings - IEEE Applied Power Electronics Conference and Exposition - APEC},
pages = {335--339},
publisher = {IEEE},
title = {{Low-volume buck converter with adaptive inductor core biasing}},
