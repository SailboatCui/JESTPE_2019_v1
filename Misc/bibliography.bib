journal = {IEEE Transactions on Power Systems},
keywords = {Atmospheric modeling,Building occupant comfort,Buildings,Frequency control,Heat pumps,Mathematical model,Water heating,direct load control (DLC),grid frequency regulation (GFR),indoor air temperature,long-term device performance,small signal analysis,variable speed heat pump (VSHP)},
pages = {1--5},
title = {{Modeling and Analysis of a Variable Speed Heat Pump for Frequency Regulation Through Direct Load Control}},
year = {2014}
}
@article{Li2010,
abstract = {Constant on-time current-mode control has been widely used to improve light-load efficiency, because it can reduce the switching frequency to save switching-related loss. Therefore, an accurate model for constant on-time control is indispensable to system design. However, available models for constant on-time control are unable to provide accurate physical insight or predict system response very well. This paper introduces a new modeling approach for constant on-time control. The inductor, the switches, and the pulsewidth-modulated modulator are treated as a single entity and modeled based on the describing function method. The fundamental difference between constant on-time control and constant-frequency peak-current-mode control is analyzed through the proposed model. This proposed modeling method can be easily extended to other current-mode controls, including V{\textless}sup{\textgreater}2{\textless}/sup{\textgreater} controls. A simple equivalent circuit representation is proposed for the sake of easy understanding and simulation of current-mode controls. Simulation and experimental results are used to verify the proposed model.},
author = {Li, J and Lee, F C},
doi = {10.1109/TPEL.2010.2040123},
file = {:C$\backslash$:/Users/richa/Google Drive (cuixf@umich.edu)/Reference/Li, Lee/IEEE Transactions on Power Electronics/Li, Lee - 2010 - IEEE Transactions on Power Electronics.pdf:pdf},
issn = {0885-8993},
journal = {IEEE Transactions on Power Electronics},
keywords = {Current-mode control,circuit switching,constant on-time control,constant-frequency peak-current-mode control,current-mode circuits,describing function (DF),describing function method,describing functions,electric current control,equivalent circuit representation,equivalent circuits,inductor,inductors,integrated circuit modelling,light-load efficiency,modeling,pulse width modulation,pulsewidth-modulated modulator,switching frequency,switching-related loss,system design},
number = {5},
