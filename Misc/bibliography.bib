journal = {IEEE Transactions on Power Delivery},
keywords = {Coordinated voltage control,distributed generation,online tap changer,reactive power management,voltage control and transient stability margin},
number = {3},
pages = {1157--1167},
title = {{A dynamic master/slave reactive power-management scheme for smart grids with distributed generation}},
volume = {29},
year = {2014}
