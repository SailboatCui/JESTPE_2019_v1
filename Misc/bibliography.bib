journal = {IEEE Transactions on Power Electronics},
keywords = {Buck converter,V 2 control,digital control,inductor current estimation,ripple-based control,small-ESR capacitors},
number = {3},
pages = {1241--1252},
title = {{Digital enhanced V 2-type constant on-time control using inductor current ramp estimation for a buck converter with low-ESR capacitors}},
volume = {28},
year = {2013}
}
