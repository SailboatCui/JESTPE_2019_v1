issn = {1930-529X},
journal = {GLOBECOM - IEEE Global Telecommunications Conference},
keywords = {Detecting Human Blockage and Device Movement in mm},
title = {{Detecting human blockage and device movement in mmwave communication system}},
year = {2011}
}
