isbn = {9781479940325},
issn = {02780046},
journal = {IECON Proceedings (Industrial Electronics Conference)},
keywords = {Digital control,current control,hysteresis control,non-linear filters},
pages = {1090--1096},
title = {{A non-linear wide bandwidth digital current controller for DC-DC and DC-AC converters}},
volume = {1},
year = {2014}
}
