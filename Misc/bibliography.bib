keywords = {Fault propagation,hierarchical control,power flow control,rotating transformer,series dynamic breaking resistor (SDBR),variable frequency transformer (VFT)},
number = {1},
pages = {132--141},
title = {{A hierarchical control strategy with fault ride-through capability for variable frequency transformer}},
volume = {30},
year = {2015}
}
