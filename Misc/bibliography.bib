@article{Budhia2013,
abstract = {Inductive power transfer is a practical method for recharging electric vehicles because it is safe, convenient, and reliable. The performance of the magnetic couplers that transfer power determines the overall feasibility of a complete system. Circular couplers are the most common topology in the literature; however, they have fundamentally limited coupling. Their flux patterns necessarily limit the operational air gap as well as tolerance to horizontal misalignment. A new polarized coupler topology [referred to as a double D (DD)] is presented, which overcomes these difficulties. DDs provide a charge zone five times larger than that possible with circular pads for a similar material cost and are smaller. A 0.31-{\textless}formula formulatype="inline"{\textgreater}{\textless}tex Notation="TeX"{\textgreater}{\$}hbox{\{}m{\}}{\^{}}{\{}2{\}}{\$} {\textless}/tex{\textgreater}{\textless}/formula{\textgreater} DD enables 2 kW of power transfer over an oval area measuring 540 mm {\textless}formula formulatype="inline"{\textgreater} {\textless}tex Notation="TeX"{\textgreater}{\$}times{\$}{\textless}/tex{\textgreater}{\textless}/formula{\textgreater} 800 mm with a 200-mm air gap. Leakage magnetic fields have been investigated and show that circular and DD couplers operating under similar power transfer conditions produce similar levels. Both topologies can be designed and operated to ensure compliance with international guidelines.},
author = {Budhia, Mickel and Boys, John T. and Covic, Grant A. and Huang, Chang Yu},
